<script>
	
	// Flex slider 
			$('.flexslider').flexslider({
				animation: "fade",
				controlNav: false,
				directionNav: false,
				pauseOnHover: false,
				slideshowSpeed: 8000
			});

			// Box animation 
			$('.content-d').hide();
			$('.box-d a').click(function() {
				var x =  $(this).attr("id");
				var $item = $('.' + x);
				if (!$item.is(":visible")) $('.content-d').hide('10','swing');
				$($item).slideToggle(400,'jswing');
				return false;
			});

			// Tooltip
			$('.tip').tooltip();

			//$("a[class^='prettyPhoto']").fancybox();
    $("a.top-go").click(function(e){
